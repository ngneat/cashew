{
  "name": "cashew-playground",
  "scripts": {
    "release": "cd projects/ngneat/cashew && standard-version --infile ../../../CHANGELOG.md",
    "contributors:add": "all-contributors add",
    "build": "ng build --configuration production",
    "build:lib": "ng build @ngneat/cashew --configuration production",
    "start": "ng serve",
    "test": "npm run test:lib",
    "test:lib": "npm run test --prefix projects/ngneat/cashew --runInBand",
    "test:lib:watch": "npm run test:watch --prefix projects/ngneat/cashew",
    "ng": "ng",
    "commit": "git-cz",
    "postbuild:lib": "cp README.md dist/ngneat/cashew",
    "prepare": "husky install",
    "pre-commit": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@angular/animations": "~13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "debug": "^2.6.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@angular/language-service": "^13.0.2",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "27.0.3",
    "all-contributors-cli": "^6.13.0",
    "codelyzer": "^6.0.2",
    "cross-env": "^7.0.3",
    "git-cz": "^4.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.0",
    "jest": "27.3.1",
    "jest-preset-angular": "11.0.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^13.0.7",
    "prettier": "^2.3.1",
    "standard-version": "^8.0.1",
    "ts-jest": "27.0.3",
    "ts-node": "~7.0.0",
    "typescript": "~4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
